#ifndef P4EST_CONFIG_H
#define P4EST_CONFIG_H 1


/* Define to the full name of this package. */
#define P4EST_PACKAGE_NAME "@package@"

/* Define to the full name and version of this package. */
#define P4EST_PACKAGE_STRING "@package@ @version@"

/* Define to the version of this package. */
#define P4EST_PACKAGE_VERSION "@version@"

/* Version number of package */
#define P4EST_VERSION "@version@"

/* Package major version */
#define P4EST_VERSION_MAJOR @version_major@

/* Package minor version */
#define P4EST_VERSION_MINOR @version_minor@

/* Package point version */
#define P4EST_VERSION_POINT @version_revision@

/* C compiler */
#define P4EST_CC "@cc@"

/* C compiler flags */
#define P4EST_CFLAGS "@cflags@"

/* C preprocessor */
#define P4EST_CPP "@cpp@"

/* C preprocessor flags */
#define P4EST_CPPFLAGS "@cppflags@"

/* Linker flags */
#define P4EST_LDFLAGS "@ldflags@"

/* Libraries */
#define P4EST_LIBS "@libs@"


/* P4EST_CONFIG_H */
#endif
